.pr_upld{position: relative;}
.pr_upld .profile-pic {max-width: 200px;max-height: 200px;display: block;}
.pr_upld .file-upload { opacity: 0;width: 100%; height: 100%;position: relative; z-index: 2; cursor: pointer;}
.pr_upld .circle {border-radius: 1000px !important;overflow: hidden;width: 130px;height: 130px;border: 3px solid #ccc;background: #000;}
.crimg_box:hover img{opacity: 0.5;}
.pr_upld img {max-width: 100%;height: auto;}
.pr_upld .p-image {position: absolute;top: 0; bottom: 0;  left: 10px; right: 0; width: 130px; height: 130px;border-radius: 100%;}
.pr_upld .p-image:hover {transition: all .3s cubic-bezier(.175, .885, .32, 1.275);}
.pr_upld .upload-button {font-size: 1.2em;}
.pr_upld .upload-button:hover {transition: all .3s cubic-bezier(.175, .885, .32, 1.275);color: #999;}
.pr_upld .p-image span{position: absolute;left: 0;right: 0;text-align: center;top: 38%;font-size: 22px;color: #fff; opacity: 0;}
.pr_upld .p-image:hover span{opacity: 1;} 

