/*========== ACCORDI0N ===========*/
.accordion_3{ margin:auto; width:100%}
.accordion_3 li{ position:relative}
.accordion_3 .accordion-item{ position:relative; margin:20px 0}
.accordion_3 .accordion-item-active .accordion-header{transition:.25s;}
.accordion_3 .accordion-header{z-index:1;cursor: pointer;padding:0px;transition: all 0.25s ease 0s;}
.accordion_3 .accordion-content{width:100%;display:none;}
.accordion_3 .accordion-header-icon{ background: url(../images/hp-b.png) no-repeat center; width:20px; height:20px; position:absolute;display:block; top:11px; right:0; padding:0 20px;background-size: 16px; }
.accordion_3 .accordion-header-icon.accordion-header-icon-active{-webkit-transform:rotate(180deg);-ms-transform:rotate(180deg);transform:rotate(180deg); background:url(../images/hm-b.png) no-repeat center; width:20px; height:20px; background-size: 16px;}
.accordion-header-icon{ font-size:0}


.accordion_2 .accordion-header{ display:none}
.accordion_2 .accordion-header{left:0;right:0;width:70px;border-radius:100px;text-align:center;height:70px;background:#d2232a; cursor:pointer;position:absolute; z-index:1; top:-20px; left:0; right:0}
.minf_h{ cursor:pointer}
.accordion_2 .accordion-header-icon{  display:block}
.accordion_2 .accordion-content{ display:block}
.accordion_2 .accordion-header-icon{margin:-67px auto auto; float:none;font-size:0px;background:url(../images/acd1_f.png); width:13px; height:13px;background-position: center;background-size: cover; }
.accordion_2 .accordion-header-icon-active{background:url(../images/acd2.png);width:13px; height:13px;background-position: center;background-size: cover; }




.accordion_faq{ margin:auto; width:95%}
.accordion_faq li{ position:relative ; padding: 0 0 10px 0; margin-bottom:10px; border-bottom: 1px solid #ccc; }

.accordion_faq li:last-child{border-bottom: 0px solid #ccc; }

.accordion_faq .accordion-item{ position:relative; margin:20px 0}
.accordion_faq .accordion-item-active .accordion-header{transition:.25s;}
.accordion_faq .accordion-header{z-index:1;cursor: pointer;padding:0px;transition: all 0.25s ease 0s; clear: both;}
.accordion_faq .accordion-content{width:100%; display:none; clear: both; padding:0 0 0 58px; position: relative;}
.accordion_faq .accordion-header-icon{ background: url(../images/ac-p1.png) no-repeat center; width:24px; height:24px; position:absolute;display:block; top:2px; right:0; padding:0 20px; }
.accordion_faq .accordion-header-icon.accordion-header-icon-active{-webkit-transform:rotate(180deg);-ms-transform:rotate(180deg);transform:rotate(180deg); background:url(../images/ac-m1.png) no-repeat center; width:24px; height:24px; }
.accordion-header-icon{ font-size:0}
.accordion_faq .accordion-content table {border-collapse: collapse; margin:10px 0}
.accordion_faq .accordion-content table tr td{border: 1px solid; padding: 10px 20px; font-size: 16px}
.accordion_faq .accordion-content a{ color:#b72f26 }
.accordion_faq a{ cursor: pointer; }


.accordion_faq .ques{ width:45px; display: inline-block; float: left; font-weight: bold; /*font-family: 'bg_bold';*/}
.accordion_faq .ans{ width: 45px; display:block; position: absolute;left:10px; font-weight: bold;}

.accordion_faq p.ques-txt{ width: auto;padding: 0 40px 2px 0; font-weight: bold; /*font-family: 'bg_bold';*/ }
.accordion_faq p{ width: auto;}
.accordion_faq .accordion-content::after{ clear: both; content: '' }
 .accordion_faq .accordion-content ul{ padding:3px 17px; margin: auto; }
 .accordion_faq .accordion-content ul li{ list-style-type: disc; margin: auto;padding: 0; border: none; }

.accordion_4{ margin:auto; width:100%}
.accordion_4  .acc-row{ position:relative ; padding-bottom: 10px: margin-bottom10px; border-bottom: 1px solid #ccc }
.accordion_4 .accordion-item_tab { position:relative ;   }
.accordion_4 .accordion-item_tab{ position:relative; margin:20px 0}
.accordion_4 .accordion-item-active_tab .accordion-header_tab{transition:.25s;}
.accordion_4 .accordion-header_tab{z-index:1;cursor: pointer;padding:0px;transition: all 0.25s ease 0s;}
.accordion_4 .accordion-content_tab{width:100%;display:none;}
.accordion_4 .accordion-header-icon_tab{ background: url(../images/hp-b.png) no-repeat center; width:24px; height:24px; position:absolute;display:block; top:13px; right:10px; padding:0 20px; }
.accordion_4 .accordion-header-icon_tab.accordion-header-icon-active_tab{-webkit-transform:rotate(180deg);-ms-transform:rotate(180deg);transform:rotate(180deg); background:url(../images/hm-b.png) no-repeat center; width:24px; height:24px; }
.accordion-header-icon_tab{ font-size:0}
 .min_box-h_tab h3{ padding-right: 40px; }

.accordion_faq_min h3{text-transform: uppercase;font-weight: bold;font-size: 20px;padding: 12px ;margin: auto;}
.accordion_faq_min .accordion-content_tab ul{margin: auto;padding: 0}
.accordion_faq_min .accordion-content_tab ul li{list-style-type: none; margin: auto;padding: 0}
.accordion_faq_min .ques-txt{margin: auto;padding: 0}
.accordion_faq_min a.min_box-h{padding: 10px; display: block;}
